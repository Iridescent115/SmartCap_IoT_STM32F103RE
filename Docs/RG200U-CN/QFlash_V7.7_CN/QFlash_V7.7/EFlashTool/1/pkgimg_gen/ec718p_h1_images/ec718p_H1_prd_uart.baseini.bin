[config]
line_0_com = COM9
agbaud = 921600
filter_embedusb = 1
filter_externcom = 0
filter_explicitcom0 = 19D1
filter_explicitcom1 = 1366
path_convert_cfg_avail = 1
pathconv_to_file_basedir_pairs=agentboot:agpath

[agentboot]
;tool_basedir = 1
agpath = ..\..\agentboot_uart\agentboot.bin

[storage_cfg]
#will be update when load
format_path = format_ec718_none.json


[bootloader]
blpath = .\image_ec718\ap_bootloader.bin
blloadskip = 0
burnaddr=0xf
;system.bin file infomation
[system]
syspath =.\image_ec718\ap_demo-flash.bin
sysloadskip = 0
burnaddr = 0xf

[cp_system]
cp_syspath = .\image_ec718\cp-demo-flash.bin
cp_sysloadskip = 0
burnaddr = 0xf

[control]
detect = 1
reset = 2
atreset = at+ecrst=delay,600
atbaud = 115200
prempt_detect_time = 6
msg_waittime = 2
max_preamble_cnt = 8
lpc_recover_en = 0
pullup_qspi = 1
trc_ser = 1
rom_version= 0000000203000000
dribble_dld_en = 1

;chaim 2023.7.5 not config rftable,download in factory mode
[flexfile0]
filepath = .\MergeRfTable.bin
burnaddr = 0x3f2000

